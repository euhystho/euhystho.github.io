{
  "name": "eugene-personal-website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --config config/astro.config.mjs",
    "build": "astro build --config config/astro.config.mjs",
    "preview": "astro build --config config/astro.config.mjs && astro preview --config config/astro.config.mjs",
    "astro": "astro --config config/astro.config.mjs",
    "format": "prettier . --write",
    "test:visual": "playwright test --config=config/playwright.config.ts --project=chromium --headed",
    "test:mobile": "playwright test --config=config/playwright.config.ts --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:accessibility": "bunx @axe-core/cli http://localhost:4321",
    "lighthouse": "bunx lighthouse http://localhost:4321 --config-path=config/lighthouse.config.js --view",
    "test:performance": "concurrently -k --success first \"astro dev --port 4321\" \"wait-on http://localhost:4321 && bunx lighthouse http://localhost:4321 --config-path=config/lighthouse.config.js\""
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "@astrojs/svelte": "^7.1.0",
    "@axe-core/cli": "^4.10.2",
    "@flaticon/flaticon-uicons": "^3.3.1",
    "@tailwindcss/vite": "^4.1.9",
    "astro": "^5.9.2",
    "astro-icon": "^1.1.5",
    "concurrently": "^9.1.2",
    "lighthouse": "^12.6.1",
    "svelte": "^5.33.19",
    "tailwindcss": "^4.1.9",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@types/node": "^24.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.12"
  }
}
