{
  "name": "eugene-personal-website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "eslint \"**/*.{js,astro}\" --fix && prettier . --write",
    "test:visual": "playwright test --project=chromium --headed",
    "test:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:accessibility": "npx @axe-core/cli http://localhost:4321",
    "test:performance": "concurrently -k \"astro dev --port 4321\" \"wait-on http://localhost:4321 && npx lighthouse http://localhost:4321 --output=html\""
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.4.0",
    "@astrojs/svelte": "^7.1.0",
    "@axe-core/cli": "^4.10.2",
    "@flaticon/flaticon-uicons": "^3.3.1",
    "@tailwindcss/vite": "^4.1.7",
    "astro": "^5.10.1",
    "astro-icon": "^1.1.5",
    "concurrently": "^9.1.2",
    "lighthouse": "^12.6.1",
    "svelte": "^5.32.1",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "wait-on": "^8.0.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@types/node": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.34.0",
    "astro-eslint-parser": "^1.2.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.26.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11"
  }
}
